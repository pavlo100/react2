    "expect": "^25.1.0",
    "file-loader": "^5.1.0",
    "jsdom": "^16.2.1",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
