    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.4",
    "react": "^16.5.2",
    "react-addons-test-utils": "^15.5.0",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.0.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.3",
    "react-router-redux": "^4.0.0",
